{
  "manifest_version": 2,

  "name": "History Collector",
  "description": "Visualize your browser history.",
  "version": "1.0",

  "permissions": [
    "chrome://favicon/",
    "history"
  ],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}